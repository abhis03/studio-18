package com.mca.studio18.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
@Entity
@Table(name="enquiry")

public class Enquiry {
	
	@Column(name="enquiry_id")
	private Long enquiryid;
	
	@Column(name="enquiry_firstname")
	private Long enquiryfirstname;
	
	@Column(name="enquiry_lastname")
	private Long enquirylastname;
	
	@Column(name="enquiry_date")
	private Long enquirydate;
	
	@Column(name="enquiry_type")
	private Long enquirytype;
	
	@Column(name="enquiry_remark")
	private Long enquiryremark;
	
	@Column(name="enquiry_package")
	private Long enquirypackage;

}
